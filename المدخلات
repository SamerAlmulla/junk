package helloaap;

import java.util.Scanner;

public class hello {

	 static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
 
 
	int[] name=new int[4];
	for(int i=3;i>=0; i--)
	name[i]= reader.nextInt();
	for(int i=3;i>=0; i--)
	
	System.out.println(name[i]);
	
	
	
	
}
}

