درس 20 ف 22

package helloaap;
import java.util.Scanner;

public class hello {
	
void display(int n1){
	
	System.out.println("integer: " +n1);
		
}
void display(float n1){
	
	System.out.println("float: " +n1);
		
}
public static void main(String[] args){
	
	new hello().display(4);
	//new hello().display(4.5f);   <<for float you have to add F
}
}

