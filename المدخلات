package helloaap;

import java.util.Scanner;

public class hello {

	static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
	
for(int i=1;i<100;i=i+2){//repeat
		
		System.out.println(i);
		
	}
}
}

