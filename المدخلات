package helloaap;

public class hello {

public static void main(String[] args) {

	
   int age=24;
   String name="samer";
   double pi=3.4; 
   char type='a';
   int oldage=23;
   
   age=oldage;
   
   System.out.println("name:"+ name );
   System.out.println("age:"+age);
   
   
}
}


