د 22 ف 24

package helloaap;
import java.io.FileOutputStream;
import java.io.FileReader;
import java.io.FileWriter;
import java.util.InputMismatchException;
import java.util.Scanner;

public class hello{
	
	
	static Scanner reader = new Scanner(System.in);
	public static void main(String[] args) {
          
		try {
    int n=reader.nextInt();
	System.out.println(n);
           }
		catch(InputMismatchException ex) {    //this for catching (InputMismatchException ex)error
			System.out.println("*Input should be integer*");}
			
		// you can CATCH more errors by adding past block.
		
		catch(Exception ex) {                 //and this for catching (All types of errors).
			System.out.println("*Input should be ...*");}
		
	System.out.println("app is done");
}
}
