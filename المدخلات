درس 18 فديو 20



package helloaap;

import java.util.Scanner;

public class hello {

	static void DrawLine() {
		//this code print stars
		for(int i=1;i<5;i++) 
		{
		     System.out.println("*");
		     for(int j=0;j<i;j++)
		     
		    	 System.out.print(" ");}
        //------------------------
	}
	//static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
	
 //to repeat STARS CODE just repeat this Line 
new hello ().	DrawLine();
 //------------------------------------------
	
	
  
  
  
  
}
}


