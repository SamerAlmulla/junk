package helloaap;

import java.util.Scanner;

public class hello {

	static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
 
	int year;
	year=reader.nextInt();
	
	int age=2017-year;
	
	System.out.println("age="+ age);
	
}
}


