package helloaap;

import java.util.Scanner;

public class hello {

	static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
	
  int i=0;
 
  do {
	  
	  System.out.println(i);
	 
	  i++;
 
  }while(i<10);
  
  
  
  
}
}

