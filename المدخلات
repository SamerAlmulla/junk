د21 ف 23


package helloaap;
import java.util.Scanner;

public class hello {
	
	static final double pi=3.4;
	

void display(float n1){
	float n2=n1+1;
	System.out.println("float: "+ n2*pi);
	
		
}
    //static Scanner reader = new Scanner(System.in);
public static void main(String[] args){
	new hello().display(4);
	//new hello().display(4.5f);  // <<for float you have to add F
	System.out.println(pi);
}
}

