package helloaap;

import java.util.Scanner;

public class hello {

	static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
	
  int i=3;
  while((i>=2)&& (i<=10))
   {
     System.out.println(i);
     i++;
   }
  
  
  
  
}
}


