package helloaap;

import java.util.Scanner;

public class hello {

	static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
	
	int age;
	age=reader.nextInt();
	
	if ( age>=18) {
		//code only execute when condition is true
		System.out.println("Welcome into the collage");
		
		
	}
	else
	{
		//code only execute when condition is not true
	System.out.println("Welcome into the school");
	
	}
	System.out.println("opp is done");
}
}



