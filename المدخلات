package helloaap;

import java.util.Scanner;

public class hello {

	static int sum(int n1, int n2) {
		int r=n1+n2;
		//you can add HERE more Codes..
		return(r);
	}
	
	//static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
	

	int result=   sum(3,5)  ;
	System.out.println(result);
	
  
  
  
  
}
}
