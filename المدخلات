package helloaap;

import java.util.Scanner;

public class hello {

	// static Scanner reader = new Scanner(System.in);
public static void main(String[] args) {
 
 
	String[] name=new String[4];
	name[0]="Samer";
	name[1]="mohammad";
	name[2]="hashim";
	name[3]="almulla";
	
	for(int i=3;i>=0; i--)
	
	System.out.println(name[i]);
	
	
	
	
}
}

